{
  "name": "gamer-ai-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "The ultimate AI gaming companion - your personal gaming oracle for Destiny 2, Path of Exile, Diablo, WoW and more!",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:fullstack": "concurrently \"npm run dev\" \"cd ../api && poetry run uvicorn app.main:app --reload --host 0.0.0.0 --port 4000\"",
    "install:all": "npm install && cd ../api && poetry install"
  },
  "keywords": [
    "gaming",
    "ai",
    "destiny-2",
    "path-of-exile",
    "diablo",
    "world-of-warcraft",
    "rag",
    "chatbot",
    "frontend",
    "react",
    "typescript"
  ],
  "author": "Gamer AI Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.0.0",
    "@tailwindcss/typography": "^0.5.19",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.18.1",
    "graphql": "^16.11.0",
    "lucide-react": "^0.516.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
